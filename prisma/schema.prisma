generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model accounts {
  id             Int       @id @default(autoincrement())
  username       String    @unique
  highscore      Int       @default(0) @db.SmallInt
  password       String
  date_completed DateTime? @db.Timestamp(6)
}
